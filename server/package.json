    "@prisma/client": "^4.16.2",
    "@sentry/node": "^7.120.0",
    "@sentry/tracing": "^7.120.3",
    "@sentry/profiling-node": "^1.3.0",
    "@types/multer": "^1.4.12",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.5.0",
    "multer": "^1.4.5-lts.2",
    "prisma": "^4.11.0",
    "qrcode": "^1.5.4",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
